    "%matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x = np.linspace(0, 10, 100)\n",
    "y = np.sin(x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.title('Simple Plot')\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('sin(x)')\n"
   ]
